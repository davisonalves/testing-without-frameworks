            "$ref": "#/parameters/_id"
          },
          {
            "$ref": "#/parameters/nome"
          },
          {
            "$ref": "#/parameters/email"
          },
          {
            "$ref": "#/parameters/password"
          },
          {
            "$ref": "#/parameters/administrador"
          }
