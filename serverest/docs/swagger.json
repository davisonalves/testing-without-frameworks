        "responses": {
          "200": {
            "description": "Lista de usuários",
            "schema": {
              "$ref": "#/definitions/getUsuarios"
            }
          }
        }
      },
      "post": {
        "tags": [
          "Usuários"
        ],
        "summary": "Cadastrar usuário",
        "description": "Não é permitido cadastrar usuário com email já utilizado",
        "parameters": [
          {
            "in": "body",
            "name": "user",
            "schema": {
              "$ref": "#/definitions/requestBodyUsuarios"
